// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model projects {
  id   Int                   @id @default(autoincrement())
  title String
  tools String
  description String
  image String
  url String
  repo_url String
}
